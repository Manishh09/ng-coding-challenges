{
  "extends": "../../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "../../../dist/out-tsc/lib",
    "declaration": true,
    "declarationMap": true,
    "inlineSources": true,
    "types": [],
    "composite": true,
    "forceConsistentCasingInFileNames": true
  },
  "angularCompilerOptions": {
    "enableIvy": true,
    "compilationMode": "partial"
  },
  "exclude": [
    "**/*.spec.ts"
  ],
  "include": [
    "src/**/*.ts"
  ]
}
