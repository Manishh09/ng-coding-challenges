{
  "name": "ng-coding-challenges",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:challenges": "ng serve coding-challenges",
    "build:challenges": "ng build coding-challenges",
    "build:libs": "ng build shared-models && ng build shared-services && ng build shared-ui",
    "build:all": "npm run build:libs && npm run build:challenges",
    "build:models": "ng build shared-models",
    "build:services": "ng build shared-services",
    "build:ui": "ng build shared-ui",
    "build": "npm run build:libs && npm run build:challenges",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "create:challenge": "node scripts/create-challenge.js",
    "start:all": "concurrently \"npm run build:libs\" \"npm run start:challenges\"",
    "test:all": "ng test --projects=shared-ui,shared-models,shared-services,coding-challenges"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^19.2.19",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/material": "^19.2.19",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.15",
    "@angular/cli": "^19.2.12",
    "@angular/compiler-cli": "^19.2.0",
    "@types/jasmine": "~5.1.0",
    "concurrently": "^8.2.2",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^19.2.2",
    "typescript": "~5.7.2"
  },

  "resolutions": {
    "wcwidth": "^1.0.1"
  },
  "overrides": {
    "wcwidth": "^1.0.1"
  }
}
